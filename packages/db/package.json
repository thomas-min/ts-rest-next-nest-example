{
  "name": "@ts-rest-example/db",
  "version": "0.0.0",
  "main": "dist/src/index.js",
  "license": "MIT",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "start:dev": "tsc --watch",
    "build": "tsc --project . && cp -rf src/generated dist/src/generated",
    "lint": "eslint 'src/**/*.ts' --max-warnings 0",
    "migrate:local": "dotenv -e .env.local prisma migrate dev",
    "migrate:dev": "dotenv -e .env.development prisma migrate deploy",
    "migrate:prod": "dotenv -e .env.production prisma migrate deploy",
    "init:db:local": "mysql -u root -e 'DROP DATABASE IF EXISTS inspiration; CREATE DATABASE inspiration CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci;'",
    "deploy:dev": "npm publish && ../../../scripts/slack_success.sh dev || ../../../scripts/slack_fail.sh dev",
    "deploy:prod": "npm publish && ../../../scripts/slack_success.sh prod || ../../../scripts/slack_fail.sh prod"
  },
  "dependencies": {
    "@prisma/client": "^4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "prisma": "^4",
    "typescript": "^4.5.2"
  },
  "files": [
    "dist",
    "prisma"
  ]
}
